{
  "name": "termlist-web",
  "description": "Web frontend for termlist-server",
  "version": "1.1.0",
  "private": true,
  "author": "Patrick Sletvold",
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run dev",
    "lint": "npm run lint:js --silent && npm run lint:css --silent",
    "lint:js": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "lint:css": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,vue,css,scss, mdx}\"",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "rm -rf node_modules/@types/react && xstate typegen \"./src/**/*.ts?(x)\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@vueuse/core": "^9.13.0",
    "bulma": "^0.9.4",
    "core-js": "^3.29.1",
    "deepmerge": "^4.3.0",
    "firebase": "^9.17.2",
    "just-compare": "^2.3.0",
    "just-debounce-it": "^3.2.0",
    "pinia": "^2.0.33",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "vue": "^3.2.47",
    "xstate": "^4.37.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@relative-ci/agent": "^3.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/vue3": "^6.5.16",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@xstate/cli": "^0.4.2",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-storybook": "^0.5.3",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "2.8.4",
    "rollup-plugin-webpack-stats": "^0.2.0",
    "sass": "^1.59.3",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
