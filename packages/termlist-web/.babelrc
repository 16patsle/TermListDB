{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "modules": false,
        "shippedProposals": true
      }
    ]
  ],
  "plugins": [
    ["@babel/proposal-decorators", { "legacy": true }]
  ]
}
